# conexión JDBC a la base (dominio y puerto)
SPRING_DATASOURCE_URL=jdbc:postgresql://dominio:31815/nombreesquema

# usuario de conexión a la base, ejemplo: postgres
SPRING_DATASOURCE_USERNAME=postgres

# password del usuario de conexión a la base
SPRING_DATASOURCE_PASSWORD=passworddeusuariopostgres

# tipo de control que hace JPA sobre el esquema (create, update, validate, none)
SPRING_JPA_HIBERNATE_DDL_AUTO=update

# mostrar las consultas SQL por consola (true/false)
SPRING_JPA_SHOW_SQL=true

# clave secreta para firmar los tokens JWT (mínimo recomendado: 32 caracteres para HS256)
JWT_SECRET=miclavesupersecreta12345678901234567890

#local





